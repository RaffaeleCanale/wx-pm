
########## PATHS ##########

export GOPATH="$HOME/Others/Installations/Go"
export SCRIPTS_ROOT="__ROOT__"
export PATH="__PATH__:$PATH:$(go env GOPATH)/bin"


########## BASH COMPLETION ##########

if [ -d "$HOME/.bash_completion.d" ]; then
    for comp in "$HOME/.bash_completion.d/"*; do
        . "$comp"
    done
fi
